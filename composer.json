{
    "name": "honeyfund/test-php",
    "description": "PHP Interview Test",
    "keywords": ["php"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/php-code-coverage": "^5.2",
        "phpunit/phpunit": "~6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
